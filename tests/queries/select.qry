<?xml version="1.0"?>
<!DOCTYPE plan SYSTEM "queryplan.dtd">

<!-- This is a query to test select -->

<plan top="select">

<dtdscan id="dtd">
<url value="http://www.cse.ogi.edu/~tufte/xml/numbers.xml"/>
</dtdscan>

<unnest id="scan" regexp="PLAY.NUM" type="content" input="dtd"/>

<select id="select" input="scan">
<pred op="ne"><var value="$scan"/><string value="5"/></pred>
</select>

</plan>
