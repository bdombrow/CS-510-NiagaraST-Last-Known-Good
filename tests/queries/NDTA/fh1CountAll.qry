<?xml version="1.0"?>
<!DOCTYPE plan SYSTEM "queryplan.dtd">

<plan top="count">
<!--First, get a count for each incoming source IP -->
<firehosescan id="fhTrace" datatype="packet" rate="0"
	host="localhost" port="5000" streaming="yes"
	desc="/local/Niagara/NiagaraCVS/firehose_1_0/mashmaro/mashmaro.trace:IP:SI:IP:TCP:TCP:HTTP:HTTP:dst:port:80"
	desc2="false" num_gen_calls="-1" num_tl_elts="100"/>
<scan id="inp" regexp="PS.P" input="fhTrace"/>
<count id="count" groupby="$inp" countattr="$inp" input="inp"/>

</plan>

