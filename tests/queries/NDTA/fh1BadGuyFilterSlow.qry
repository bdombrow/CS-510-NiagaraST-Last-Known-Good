<?xml version="1.0"?>
<!DOCTYPE plan SYSTEM "queryplan.dtd">

<plan top="select">
<!--First, get a count for each incoming source IP -->
<firehosescan id="fhTrace" datatype="packet" rate="0"
	host="localhost" port="5000" streaming="yes"
	desc="/local/Niagara/NiagaraCVS/firehose_1_0/mashmaro/mashmaro.trace"
	desc2="false" num_gen_calls="1" num_tl_elts="10"/>
<scan id="inip" regexp="PS.P.IP.SI" input="fhTrace"/>
<select id="select" input="inip">
   <or>
      <pred op="eq">
         <var value="$inip"/><string value="12.224.243.155"/>
      </pred>
      <or>
         <pred op="eq">
            <var value="$inip"/><string value="129.95.51.101"/>
         </pred>
         <or>
            <pred op="eq">
               <var value="$inip"/><string value="129.95.50.116"/>
            </pred>
            <or>
               <pred op="eq">
                  <var value="$inip"/><string value="12.250.178.44"/>
               </pred>
               <pred op="eq">
                  <var value="$inip"/><string value="68.113.32.14"/>
               </pred>
            </or>
         </or>
      </or>
   </or>
</select>

</plan>

