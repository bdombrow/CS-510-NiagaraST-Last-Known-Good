<?xml version="1.0"?>
<!DOCTYPE plan SYSTEM "queryplan.dtd">

<plan top="cons">
<dtdscan id="dtd">
<url value="http://www.cs.pdx.edu/~tufte/xml/numbers.xml"/>
</dtdscan>
<unnest id="scan" regexp="PLAY" input="dtd"/>
<unnest id="scan1" regexp="NUM" input="scan"/>
<avg id="avg" groupby="$scan" avgattr="$scan1" input="scan1"/> 
<construct id="cons" input="avg">
<![CDATA[
<Average>$avg</Average>
]]>
</construct>
</plan>
