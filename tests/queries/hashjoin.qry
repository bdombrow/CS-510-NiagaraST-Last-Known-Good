<?xml version="1.0"?>
<!DOCTYPE plan SYSTEM "queryplan.dtd">

<plan top="cons">

<dtdscan id="acdtdscan">
<url value="http://www.cs.pdx.edu/~tufte/xml/a_and_c.xml"></url>
</dtdscan>

<dtdscan id="jcdtdscan">
<url value="http://www.cs.pdx.edu/~tufte/xml/j_caesar.xml"></url>
</dtdscan>

<unnest root="" type="content" 
        regexp="PLAY.PERSONAE.PGROUP.PERSONA|PLAY.PERSONAE.PERSONA" 
	  id="acscan" 
          input="acdtdscan"/>

<unnest root="" type="content" 
        regexp="PLAY.PERSONAE.PGROUP.PERSONA|PLAY.PERSONAE.PERSONA" 
          id="jcscan" 
          input="jcdtdscan"/>

<join id="join" left="$acscan" right="$jcscan" 
          input="acscan jcscan">
</join>

<construct id="cons" input="join">
<![CDATA[ <PERSONA>$acscan</PERSONA> ]]>
</construct>
</plan>
