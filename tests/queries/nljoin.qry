<?xml version="1.0"?>
<!DOCTYPE plan SYSTEM "queryplan.dtd">

<plan top="cons">

<dtdscan id="acdtdscan">
<url value="http://www.cse.ogi.edu/~tufte/xml/a_and_c.xml"></url>
</dtdscan>

<dtdscan id="jcdtdscan">
<url value="http://www.cse.ogi.edu/~tufte/xml/j_caesar.xml"></url>
</dtdscan>

<unnest root="" type="content" 
        regexp="PLAY.PERSONAE.PGROUP.PERSONA|PLAY.PERSONAE.PERSONA" 
	  id="acscan" 
          input="acdtdscan"/>

<unnest root="" type="content" 
        regexp="PLAY.PERSONAE.PGROUP.PERSONA|PLAY.PERSONAE.PERSONA" 
          id="jcscan" 
          input="jcdtdscan"/>

<join  id="join" input="acscan jcscan">
<pred op="eq">
       <var value="$acscan"></var><var value ="$jcscan"></var>
</pred>
</join>

<construct id="cons" input="join">
<![CDATA[ <PERSONA>$acscan</PERSONA> ]]>
</construct>
</plan>
