<?xml version="1.0"?>
<!DOCTYPE plan SYSTEM "queryplan.dtd">

<!-- Test predicates with embedded paths -->

<plan top="select">

<constant id="scan">
<![CDATA[
<list>
<item><num>3</num><num>4</num><num>4</num></item>
<item><num>-1</num><num>6</num></item>
<item/>
<item><num>6</num><num>7</num><num>8</num></item>
</list>
]]>
</constant>

<unnest id="item" regexp="list.item" input="scan"/>

<select id="select" input="item">
<pred op="gt"><path var="$item" path="num"/><number value="5"/></pred>
</select>

</plan>
