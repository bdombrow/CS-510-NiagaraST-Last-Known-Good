<?xml version="1.0"?>
<!DOCTYPE plan SYSTEM "queryplan.dtd">

<!-- This query tests having a construct operator in the middle
of the operator tree -->

<plan top="cons">

<dtdscan id="dtd">
<url value="http://www.cs.pdx.edu/~tufte/xml/numbers.xml"/>
</dtdscan>

<unnest id="scan" regexp="PLAY.NUM" type="content" input="dtd"/>

<construct id="cons" input="scan">
<![CDATA[
<newnum>$scan</newnum>
]]>
</construct> 

</plan>
