<?xml version="1.0"?>
<!DOCTYPE plan SYSTEM "queryplan.dtd">

<!-- This query tests having a construct operator in the middle
of the operator tree -->

<plan top="select">

<dtdscan id="dtd">
<url value="http://www.cse.ogi.edu/~tufte/xml/numbers.xml"/>
</dtdscan>

<unnest id="scan" regexp="PLAY.NUM" type="content" input="dtd"/>

<construct id="cons" input="scan">
<![CDATA[
<num>$scan</num>
]]>
</construct> 

<select id="select" input="cons">
<pred op="ne"><var value="$scan"/><string value="xx"/></pred>
</select>

</plan>
