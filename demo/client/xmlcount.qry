<?xml version="1.0"?>
<!DOCTYPE plan SYSTEM "queryplan.dtd">

<plan top="cons">
<dtdscan id="dtd">
<url value="http://king.cse.ogi.edu/xml/numbers.xml"/>
</dtdscan>
<scan id="scan" regexp="PLAY" input="dtd"/>
<scan id="scan1" regexp="NUM" input="scan"/>
<count id="count" groupby="$scan" countattr="$scan1" input="scan1"/>
<construct id="cons" input="count">
<Count>$count</Count>
</construct>
</plan>
