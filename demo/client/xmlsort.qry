<?xml version="1.0"?>
<!DOCTYPE plan SYSTEM "queryplan.dtd">

<plan top="sort">
<dtdscan id="dtd">
<url value="http://www.cse.ogi.edu/~vpapad/xml/numbers.xml"/>
</dtdscan>
<scan id="scan" regexp="PLAY.NUM" input="dtd"/>
<construct id="cons" input="scan">
<NUM>$scan</NUM>
</construct>
<sort id="sort" input="cons" comparison="numeric" order="ascending" sort_by="$scan"/>
</plan>
